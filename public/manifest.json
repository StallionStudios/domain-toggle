{
	"manifest_version": 3,
	"name": "Domain Toggle",
	"description": "Toggle between remote and local domains",
	"version": "1.0",
	"action": {
		"default_popup": "index.html",
		"default_icon": "production-icon.png"
	},
	"background": {
		"service_worker": "background.js"
	},
	"permissions": ["tabs", "activeTab"]
}
